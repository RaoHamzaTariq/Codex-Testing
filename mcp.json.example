{
  "servers": [
    {
      "name": "email",
      "command": "node",
      "args": ["/path/to/email-mcp/index.js"],
      "env": {"GMAIL_CREDENTIALS": "/path/to/creds.json"}
    },
    {
      "name": "browser",
      "command": "npx",
      "args": ["@anthropic/browser-mcp"],
      "env": {"HEADLESS": "true"}
    }
  ]
}
